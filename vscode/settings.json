{
  "advancedNewFile.exclude": {
    "dev": true,
    "dist": true,
    "node_modules": true,
    "node_modules_electron": false
  },
  "css.hover.references": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeLens": false,
  "editor.comments.insertSpace": false,
  "editor.cursorSurroundingLines": 10,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Iosevka SS14",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "folderTemplates.fileTemplates": {
    "TSReactFC": [
      "import React from 'react'",
      "interface <FTName>Props {",
      "}",
      "",
      "function <FTName>(props: <FTName>Props) {",
      "  return <div><FTName></div>",
      "}",
      "",
      "export default <FTName>"
    ],
    "Indexfile": "import <FTName> from './<FTName>'\n\nexport default <FTName>;",
    "ReduxReducer": [
      "import { AnyAction } from 'redux'",
      "",
      "const initialState = {}",
      "",
      "type State = typeof initialState",
      "",
      "type Payload = {}",
      "",
      "type Action = AnyAction & {",
      "  payload: Payload",
      "}",
      "",
      "const <FTName>Reducer = (state: State = initialState, action: Action) => {",
      "  switch (action.type) {",

      "    default:",
      "      return state",
      "  }",
      "}",
      "export default <FTName>Reducer"
    ]
  },
  "folderTemplates.structures": [
    {
      "name": "Typescript React Functional Component",
      "omitParentDirectory": true,

      "structure": [
        {
          "fileName": "<FTName>/<FTName>.tsx",
          "template": "TSReactFC"
        },
        {
          "fileName": "<FTName>/<FTName>.test.js"
        },
        {
          "fileName": "<FTName>/index.ts",
          "template": "Indexfile"
        }
      ]
    },
    {
      "name": "Redux",
      "customVariables": ["Extension"],
      "omitParentDirectory": true,
      "structure": [
        {
          "fileName": "<FTName>/<FTName>.actions.<Extension>"
        },
        {
          "fileName": "<FTName>/<FTName>.reducers.<Extension>",
          "template": "ReduxReducer"
        },
        {
          "fileName": "<FTName>/<FTName>.types.<Extension>"
        }
      ]
    }
  ],
  "editor.fontWeight": "400",
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.timeout": 250,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["L"],
      "after": ["$"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<esc>"]
    },
    {
      "before": ["<C-e>"],
      "commands": [
        "editor.emmet.action.expandAbbreviation",
        "extension.vim_escape",
        "extension.vim_right",
        "extension.vim_right"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    }
  ],
  "vim.normalModeKeyBindings": [
    { "before": ["<leader>", "c"], "commands": ["workbench.action.closeActiveEditor"] }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "f", "r"], "commands": ["fileutils.renameFile"] },
    { "before": ["<leader>", "f", "d"], "commands": ["fileutils.removeFile"] },
    { "before": ["<leader>", "f", "m"], "commands": ["fileutils.moveFile"] },
    { "before": ["<leader>", "f", "f"], "commands": ["workbench.action.quickOpen"] },
    { "before": ["<leader>", "t"], "commands": ["workbench.action.showAllSymbols"] },
    { "before": ["<leader>", "o"], "commands": ["workbench.action.gotoSymbol"] },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["editor.action.startFindReplaceAction"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["revealInExplorer"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["extension.advancedNewFile"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    }
  ],
  "vim.highlightedyank.enable": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-s>": false,
    "<C-z>": false,
    "<C-y>": false,
    "<C-f>": false,
    "<C-x>": false,
    "<C-t>": false,
    "<C-e>": true,
    "<C-j>": true,
    "<C-w>": true
  },
  "vim.easymotion": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.addMissingImports": true
  },
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.lightbulb.enabled": false,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.rulers": [{ "column": 120 }],
  "editor.mouseWheelScrollSensitivity": 3,
  "editor.parameterHints.enabled": false,
  "editor.stickyTabStops": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.showDeprecated": false,
  "editor.lineHeight": 24,
  "editor.suggest.showWords": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": false,
  "editor.wordSeparators": "`~!@#$%^&*()-_=+[{]}\\|;:'\",.<>/?",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "explorer.autoReveal": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onWindowChange",
  "files.exclude": {
    "**/node_modules": true
  },
  "files.associations": {
    "*.env*": "env"
  },
  "files.trimTrailingWhitespace": true,
  "gitlens.heatmap.locations": ["gutter", "overview"],
  "gitlens.hovers.currentLine.over": "line",
  "html.completion.attributeDefaultValue": "singlequotes",
  "html.hover.documentation": false,
  "html.hover.references": false,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.arrowParens": "avoid",
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 100,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "scss.hover.references": false,
  "search.exclude": {
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Iosevka Term SS14",
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.commandsToSkipShell": ["workbench.action.focusActiveEditorGroup"],
  "terminal.integrated.cursorWidth": 3,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.gpuAcceleration": "auto",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    }
  },
  "terminal.integrated.tabs.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "manual",
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.openFilesInNewWindow": "off",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "window.titleBarStyle": "custom",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.enableExperiments": false,
